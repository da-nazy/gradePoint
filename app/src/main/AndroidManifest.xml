            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5845987937730369~6847789230" />
        <activity android:name="com.app.gradepoint.login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.app.gradepoint.courseList" />
        <activity android:name="com.app.gradepoint.register">
        </activity>
    </application>

</manifest>